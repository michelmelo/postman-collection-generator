{
    "name": "michelmelo/postman-collection-generator",
    "type": "symfony-bundle",
    "description": "Generator for Postman collection based on API Platform 1.*",
    "keywords": ["Postman", "API Platform"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "eu@michelmelo.pt"
        }
    ],
    "require": {
        "php": ">=5.5",
        "symfony/console": "^2.3|^3.0|^5.1",
        "symfony/dependency-injection": "^2.3|^3.0",
        "symfony/config": "^2.3|^3.0",
        "doctrine/inflector": "^2.0",
        "api-platform/core": "~1.1@beta",
        "ramsey/uuid": "^3.2",
        "fzaninotto/faker": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "<5.0",
        "behat/behat": "~3.0",
        "behat/symfony2-extension": "~2.0",
        "behat/mink": "~1.5",
        "behat/mink-extension": "~2.0",
        "behat/mink-browserkit-driver": "~1.1",
        "behatch/contexts": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "PostmanGeneratorBundle\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
